{
    "name": "delivery-team",
    "version": "1.0.0",
    "license": "MIT",
    "private": true,
    "scripts": {
      "dev:on": "docker-compose up -d",
      "dev:off": "docker-compose down",
      "person": "cd services/01_person",
      "products": "cd services/02_products && yarn dev",
      "order": "cd services/03_order && yarn dev",
      "payment": "cd services/04_payment && yarn dev",
      "invoice": "cd services/05_invoice && yarn dev",
      "delivery": "cd services/06_delivery && yarn dev",
      "start": "concurrently --kill-others-on-fail 'yarn order' 'yarn payment'"
    },
    "dependencies": {
      "concurrently": "5.1.0"
    }
  }
  